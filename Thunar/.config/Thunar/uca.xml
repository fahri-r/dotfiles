<actions>
<action>
	<icon>utilities-terminal</icon>
	<name>Open Terminal Here</name>
	<submenu></submenu>
	<unique-id>1747175048252390-1</unique-id>
	<command>uwsm app -- alacritty --working-directory %f</command>
	<description>Instantly opens a terminal window at the current folder location for quick command-line access.</description>
	<range></range>
	<patterns>*</patterns>
	<startup-notify/>
	<directories/>
</action>
<action>
